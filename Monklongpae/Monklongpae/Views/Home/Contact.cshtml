<section id="Sec_MoneyPage" style="background-image: linear-gradient(0deg,#000000e3,transparent), url(/img/back_video.jpg);">
        <div class="container">
            <div class="Head_Video">
                <p> @Model.data.MessagePage4En </p>
            </div>
            <div class="Head_Video_Head">
                <p> @Model.data.MessagePage4Th </p>
            </div>
        </div>
    </section>


    <section id="Sec_ContactBox">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-md-12 col-12">
                    <div class="Box_ImageIcon mb-5">
                        <div class="center">
                            <img src="/img/tel.png" alt="logo" width="70px" height="70px" />
                        </div>
                    </div>
                    <div class="Text_IconText">
                        <p> @Model.data1.Tel </p>
                    </div>
                </div>
                <div class="col-lg-6 col-md-12 col-12">
                    <div class="Box_ImageIcon mb-5">
                        <div class="center">
                            <img src="/img/maps.png" alt="logo" width="70px" height="70px" />
                        </div>
                    </div>
                    <div class="Text_IconText">
                        <p> @Model.data1.Address </p>
                    </div>
                </div>
            </div>
            <div class="row mt-5">
                <div class="col-lg-12 col-md-12 col-12">
                    <iframe src="@Model.data1.Map" width="100%" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
                </div>
            </div>
        </div>
    </section>
